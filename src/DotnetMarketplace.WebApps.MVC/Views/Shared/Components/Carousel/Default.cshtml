@model List<CarouselItemViewModel>

@if (Model.Count > 0)
{
    <section id="secao-carrossel">
        <div id="carrossel-home" class="slick-slider">
        @foreach (var item in Model)
        {
            <div>
                <img src="@item.File" class="d-block w-100" alt="@item.File" title="@item.File" />
            </div>
        }
        </div>
    </section>
}